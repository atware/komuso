<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
  <head>
    <title>Packages</title>
    <meta http-equiv="Content-Type" CONTENT="text/html; charset=UTF-8">
    <script language="JavaScript" src="refine.js"></script>
    <link rel="stylesheet" type="text/css" href="stylesheet.css">
  </head>
  <body>
    <table border="0" width="100%">
      <tr>
        <td nowrap><font size="+1"><b>$message.getString("komuso")</b></font></td>
     </tr>
    </table>

    <table border="0" width="100%">
     <tr>
      <td nowrap><font class="small"><a href="allclasses-frame.html" target="packageFrame">$message.getString("allclasses")</a></font>
      <br><br>$message.getString("packages")<br>
        <input type="search" onkeyup="refine('packageName',this.value)" autosave="packageName" placeholder="$message.getString("refine")" results="5"">
        #foreach($packageName in $packageList)
        <div style="margin-top:7px" id="${packageName}" name="packageName" class="small"><a href="${packageName}.html" target="packageFrame">$packageName</a></div>
      #end
      </td>
     </tr>
   </table>
  </body>
</html>
